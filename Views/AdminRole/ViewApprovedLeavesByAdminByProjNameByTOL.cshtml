@model sampleMVC.Models.AdminRole

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>TOLandPRojectForUnplannedByAdmin</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</head>
<body>
    <script type="text/javascript">
            function preventBack() { window.history.forward(); }
            setTimeout("preventBack()", 0);
            window.onunload = function () { null };
    </script>
    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="~/AdminRole/AdminRole">Leave Tracker</a>
            </div>
            <div class="nav navbar-nav navbar-right">
                @Html.ActionLink("Logout", "Logout", "LTLogin", new { @class = "navbar-brand" })
            </div>
            <ul class="nav navbar-nav">
                @*<li><a href="#">Register New Member</a></li>
                    <li><a href="~/AdminRole/ApplyLeavesforAdmins">Apply Leave</a></li>*@
                <li><a href="~/AdminRole/ViewLeavesByAdmin">Approve Leaves</a></li>
                <li><a href="~/AdminRole/ViewApprovedLeavesByAdmin">View Approved Leaves</a></li>
                @*<li><a href="~/AdminRole/AdminMoreFeature">More Features</a></li>
                    <li><a href="~/TLRole/Logout">Logout</a></li>*@
            </ul>
        </div>
    </nav>


    <div class="container" style="padding-top:10px">
      
        @Html.ActionLink("Back", "ViewApprovedLeavesByAdmin", "AdminRole")
        <div class="table-responsive">
            @using (Html.BeginForm())
            {
                <table class="table table-striped table-bordered table-hover">

                    <tr>

                        <th>First Name</th>
                        <th>Last Leaves</th>
                        <th>Project</th>
                        <th>Branch</th>
                        <th>Location</th>
                        <th>Start Date</th>
                        <th>End Date</th>
                        <th>Type Of Leave</th>
                        <th>Reason</th>
                        <th>Status</th>

                    </tr>

                    @foreach (sampleMVC.Models.AdminRole item in Model.ViewApprovedLeavesByAdminlist)
                    {

                        <tr>
                            <td>@item.FirstName</td>
                            <td>@item.LastName</td>
                            <td>@item.ProjectName</td>
                            <td>@item.Branch</td>
                            <td>@item.Location</td>
                            <td>@item.StartDate</td>
                            <td>@item.EndDate</td>
                            <td>@item.typeOfLeave</td>
                            <td>@item.Reason</td>
                            <td>@item._Status</td>

                        </tr>
                    }
                </table>
            }
        </div>
    </div>

</body>
</html>
