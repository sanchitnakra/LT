@model sampleMVC.Models.SuperAdmin

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ExportToExcelForSuperAdmin</title>
    <style type="text/css">
        body {
            font-family: Calibri;
        }

        h2 {
            color: red;
        }

        table {
            border: 10px solid black;
            border-spacing: 0;
        }

        td {
            color: black;
            font-size: 1em;
            padding: 5px;
        }

        .heading {
            background: #1E90FF;
        }
    </style>

</head>
<body>

    <table>
        <tr class="heading">
            <th>TCS Emp ID</th>
            <th>MSID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Exp Level</th>
            <th>Silo</th>
            <th>Sub Area</th>
            <th>Location</th>
            <th>Leave Month</th>
            <th>Project</th>
            <th>Start Date</th>
            <th>End Date</th>
            <th>Type Of Leave</th>
            <th>No. of Days</th>
            <th>Reason</th>
            <th>Status</th>
            <th>Customer Manager Name</th>
        </tr>

        @foreach (sampleMVC.Models.SuperAdmin item in Model.ExportToExcelForSuperAdminList)

        {

            <tr>
                <td>@item.emp_ID</td>
                <td>@item.MSID</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.expLevel</td>
                <td>@item.Branch</td>
                <td>@item.subArea</td>
                <td>@item.Location</td>
                <td>@item.fetchYear</td>  
                <td>@item.ProjectName</td> 
                <td>@item.StartDate</td>
                <td>@item.EndDate</td>
                <td>@item.typeOfLeave</td>
                <td>@item.totalLeaves</td>
                <td>@item.Reason</td>
                <td>@item._Status</td>
                <td>@item.custManaName</td>


            </tr>
        }


    </table>
</body>
</html>
