@model sampleMVC.Models.ApplyLeave

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ExportToExcelForUser</title>
    <style type="text/css">
        body {
            font-family: Calibri;
        }

        h2 {
            color: red;
        }

        table {
            border: 10px solid black;
            border-spacing: 0;
        }

        td {
            color: black;
            font-size: 1em;
            padding: 5px;
        }

        .heading {
            background: #1E90FF;
        }

        .border {
        }
    </style>
</head>
<body>
    <table>
        <tr class="heading">
            <th>MSID</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
        @foreach (sampleMVC.Models.ApplyLeave item in Model.ExportToExcelForUserList)
        {
            <tr>
                <td>@item.MSID</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
            </tr>
        }
    </table>
</body>
</html>
